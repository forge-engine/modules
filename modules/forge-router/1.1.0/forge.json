{
    "$schema": "https://raw.githubusercontent.com/upperdo/forge-schemas/refs/heads/main/schemas/modules/schema.json",
    "name": "forge-router",
    "version": "1.1.0",
    "description": "Forge basic router example",
    "type": "router",
    "order": 4,
    "provides": [
        "RouterInterface@1.1.0"
    ],
    "requires": [
        "ViewEngineInterface@1.0"
    ],
    "lifecycleHooks": [
        "beforeBoot",
        "afterBoot",
        "beforeModuleLoad",
        "afterModuleLoad",
        "beforeModuleRegister",
        "afterModuleRegister",
        "afterConfigLoaded"
    ],
    "class": "Forge\\Modules\\Router\\RouterModule",
    "cli": {
        "commands": [
            "route:list"
        ]
    },
    "tags": [
        "rouer"
    ],
    "config": {
        "defaults": {}
    },
    "compatibility": {
        "framework": ">=0.0.1",
        "php": "^8.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/forge-engine/modules"
    }
}